            <Version>1</Version>
            <Timestamp>2003-10-11T22:14:15+00:00</Timestamp>
            <Hostname>mymachine.example.com</Hostname>
            <AppName>su</AppName>
            <ProcID xsi:nil="true" />
            <MsgID>ID47</MsgID>
          </Header>
          <Message>
            <UTF8>'su root' failed for lonvick on /dev/pts/8</UTF8>
          </Message>
        </sl:Syslog>
